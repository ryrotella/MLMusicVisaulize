{
  "name": "transcribe-audio-live",
  "version": "1.0.0",
  "description": "Real-time music visualization app that paints lyrics and mood on screen",
  "main": "youtube-proxy.js",
  "scripts": {
    "start": "python3 server.py",
    "proxy": "node youtube-proxy.js",
    "dev": "concurrently \"python3 server.py\" \"node youtube-proxy.js\"",
    "install-deps": "npm install express cors ytdl-core concurrently"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "music",
    "visualization",
    "youtube",
    "audio",
    "canvas",
    "ai",
    "transcription"
  ],
  "author": "TranscribeAudioLive",
  "license": "MIT"
}